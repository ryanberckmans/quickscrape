    "which": "~1.0.5",
    "winston": "~2.4.0"
  },
  "bin": {
    "quickscrape": "bin/quickscrape.js"
  },
  "devDependencies": {
    "coveralls": "~2.11.2",
    "grunt": "~0.4.5",
    "istanbul": "~0.3.13",
    "mocha": "~2.2.4",
    "mocha-lcov-reporter": "0.0.2",
